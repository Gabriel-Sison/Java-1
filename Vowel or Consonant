import java.util.*;

public class Exercise04_13 {
public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
	
	System.out.print("Enter a letter: ");
	
	String letter = input.next(); //65 97
	int value = letter.charAt(0); // turn string to number
	
	switch (value) {
	case 65: System.out.printf("%s is a vowel", letter); break;
	case 66: System.out.printf("%s is a consonant", letter); break;
	case 67: System.out.printf("%s is a consonant", letter); break;
	case 68: System.out.printf("%s is a consonant", letter); break;
	case 69: System.out.printf("%s is a vowel", letter); break;
	case 70: System.out.printf("%s is a consonant", letter); break;
	case 71: System.out.printf("%s is a consonant", letter); break;
	case 72: System.out.printf("%s is a consonant", letter); break;
	case 73: System.out.printf("%s is a vowel", letter); break;
	case 74: System.out.printf("%s is a consonant", letter); break;
	case 75: System.out.printf("%s is a consonant", letter); break;
	case 76: System.out.printf("%s is a consonant", letter); break;
	case 77: System.out.printf("%s is a consonant", letter); break;
	case 78: System.out.printf("%s is a consonant", letter); break;
	case 79: System.out.printf("%s is a vowel", letter); break;
	case 80: System.out.printf("%s is a consonant", letter); break;
	case 81: System.out.printf("%s is a consonant", letter); break;
	case 82: System.out.printf("%s is a consonant", letter); break;
	case 83: System.out.printf("%s is a consonant", letter); break;
	case 84: System.out.printf("%s is a consonant", letter); break;
	case 85: System.out.printf("%s is a vowel", letter); break;
	case 86: System.out.printf("%s is a consonant", letter); break;
	case 87: System.out.printf("%s is a consonant", letter); break;
	case 88: System.out.printf("%s is a consonant", letter); break;
	case 89: System.out.printf("%s is a consonant", letter); break;
	case 90: System.out.printf("%s is a consonant", letter); break;
	case 97: System.out.printf("%s is a vowel", letter); break;
	case 98: System.out.printf("%s is a consonant", letter); break;
	case 99: System.out.printf("%s is a consonant", letter); break;
	case 100: System.out.printf("%s is a consonant", letter); break;
	case 101: System.out.printf("%s is a vowel", letter); break;
	case 102: System.out.printf("%s is a consonant", letter); break;
	case 103: System.out.printf("%s is a consonant", letter); break;
	case 104: System.out.printf("%s is a consonant", letter); break;
	case 105: System.out.printf("%s is a vowel", letter); break;
	case 106: System.out.printf("%s is a consonant", letter); break;
	case 107: System.out.printf("%s is a consonant", letter); break;
	case 108: System.out.printf("%s is a consonant", letter); break;
	case 109: System.out.printf("%s is a consonant", letter); break;
	case 110: System.out.printf("%s is a consonant", letter); break;
	case 111: System.out.printf("%s is a vowel", letter); break;
	case 112: System.out.printf("%s is a consonant", letter); break;
	case 113: System.out.printf("%s is a consonant", letter); break;
	case 114: System.out.printf("%s is a consonant", letter); break;
	case 115: System.out.printf("%s is a consonant", letter); break;
	case 116: System.out.printf("%s is a consonant", letter); break;
	case 117: System.out.printf("%s is a vowel", letter); break;
	case 118: System.out.printf("%s is a consonant", letter); break;
	case 119: System.out.printf("%s is a consonant", letter); break;
	case 120: System.out.printf("%s is a consonant", letter); break;
	case 121: System.out.printf("%s is a consonant", letter); break;
	case 122: System.out.printf("%s is a consonant", letter); break;
	default: System.out.println("# is an invalid input");
	}
